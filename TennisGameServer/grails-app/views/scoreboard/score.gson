import tennisgame.Match
import tennisgame.TennisGames

model {
    Match match
}

json {
    msg match.lastEvent
//    service match.servicePlayer
//    winner match.winner

    score(
        player0: match.score.points[0],
        player1: match.score.points[1]
    )
    sets match.sets, { TennisGames set ->
        player0 set.games[0]
        player1 set.games[1]
    }
}